  "support": {
    "issues": "https://github.com/franzholz/base_font/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
        "name": "Fabien MÃ©nager",
        "email": "fabien.menager@gmail.com"
    },
    {
      "name": "Franz Holzinger",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": ">=6.2.0,<9.99.0",
    "phenx/php-font-lib": ">=0.3.1,<1"
  },
  "repositories": [
    {
        "type": "path",
        "url": "Vendor/php-font-lib"
    }
