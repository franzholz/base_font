{
  "name": "jambagecom/base-font",
  "type": "typo3-cms-extension",
  "description": "This provides the PHP Font Lib.",
  "keywords": [
    "TYPO3",
    "extension",
    "font",
    "phpOffice"
  ],
  "support": {
    "issues": "https://github.com/franzholz/base_font/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
        "name": "Fabien MÃ©nager",
        "email": "fabien.menager@gmail.com"
    },
    {
      "name": "Franz Holzinger",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms-core": ">=6.2.0,<9.99.0"
  },
  "autoload": {
    "psr-4": {
      "FontLib\\": "Vendor/php-font-lib/src/FontLib"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "replace": {
    "base_font": "self.version",
    "typo3-ter/base-font": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "base_font"
    }
  }
}
